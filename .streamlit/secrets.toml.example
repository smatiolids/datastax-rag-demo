# OpenAI secrets
OPENAI_API_KEY = ""

# Optionally: LangSmith secrets for tracing
LANGCHAIN_ENDPOINT = ""
LANGCHAIN_API_KEY = ""
LANGCHAIN_PROJECT = ""

# Astra Vector Secrets
ASTRA_VECTOR_ENDPOINT = ""
ASTRA_VECTOR_TOKEN = ""

[passwords]
# Follow the rule: username = "password"
# Where username defines the Astra Vector Table and the rails.csv experience
postnl = "password"
datastax = "password"

# Astra Vector Endpoint
# For each user it is needed to define the ASTRA ENDPOINT (get it on the Astra UI)
[astra_db_endpoint]
postnl = "https://....apps.astra.datastax.com"
datastax = "https://....apps.astra.datastax.com"

#Astra Vector Token
[astra_db_token]
postnl = "AstraCS:..."
datastax = "AstraCS:..."

[languages]
# Follow the rule: username = "localization_code"
# Where localization_code defines the language from localization.csv
postnl = "nl_NL"
datastax = "en_US"

[delete_option]
# Follow the rule: username = "delete_option"
# Where delete_option = True allows the 'Delete Context' button to be shown for that username
datastax = "True"